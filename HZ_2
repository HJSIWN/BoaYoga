			pcall(function()
				humanoid.WalkSpeed = tonumber(value)
			end)
		end
	})

	Tabs.Mode1:Slider({
		Title = Translate("พลังการกระโดด", "Jump Power"),
		Value = {
			Min = 0,
			Max = 50,
			Default = 7.2,
		},
		Step = 0.5,
		Callback = function(value)
			pcall(function()
				humanoid.JumpHeight = value
			end)
		end
	})
	
end)

