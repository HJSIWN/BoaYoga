	Title = Translate("‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•", "Auto Use Skill"),
	Value = false,
	Callback = function(state)
		AutoUseSkill = state
	end
})

