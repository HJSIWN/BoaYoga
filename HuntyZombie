	Title = Translate("‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò", "Weapon Skill"),
	Value = true,
	Callback = function(state)
		WeaponSkill = state
	end
})

