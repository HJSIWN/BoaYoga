				end
				cameraPos = HMNRT.Position + Vector3.new(0, 15, 0)
				lookAtPos = cameraPos + Vector3.new(0, 100, 0)
				Camera.CameraType = Enum.CameraType.Scriptable
				Camera.CFrame = CFrame.new(cameraPos, lookAtPos)
			end
		end)
	end
end)

