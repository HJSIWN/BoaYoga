end
Window:SelectTab(1)
task.spawn(function()
	while true do
		pcall(function()
			character = player.Character
			humanoid = character:FindFirstChildOfClass("Humanoid")
			HMNRT = character:FindFirstChild("HumanoidRootPart")
		end)
		wait(1)
	end
end)

