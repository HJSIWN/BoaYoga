	Animals = workspace:WaitForChild("Gameplay"):WaitForChild("Dynamic"):WaitForChild("Animals"),
	Lobby_CFrame = CFrame.new(-2, 51, 220, 1, 0, 0, 0, 1, 0, 0, 0, 1),
	Game_CFrame = CFrame.new(8, 14, 89, -1, 5, 0, 0, 1, 0, 0, 0, -1),
}

