Title_Info2 = Tabs.Mode2:Section({Title = "Section",TextXAlignment = "Left",TextSize = 30,})
Title_Info2:SetTitle("Esp")

Esp = false
Tabs.Mode2:Toggle({
	Title = "Esp | มองทะลุ",
	Value = false,
	Callback = function(state)
		Esp = state
		if not Esp then
			wait(0.5)
			pcall(function()
				for _, Model in ipairs(workspace:WaitForChild("Gameplay"):WaitForChild("Dynamic"):WaitForChild("Animals"):GetChildren()) do
					if Model:FindFirstChild("SomtankESP") then
						Model.SomtankESP:Destroy()
					end
				end
			end)
		end
	end
})

Title_Info3 = Tabs.Mode2:Section({Title = "Section",TextXAlignment = "Left",TextSize = 30,})
Title_Info3:SetTitle("เพิ่มเติม")

Tabs.Mode2:Button({
	Title = "ไอเท็มวาร์ป",
	Desc = "Tptool",
	Callback = function()
		pcall(function()
			local TpTool = Instance.new("Tool")
			TpTool.Name = "วาร์ป"
			TpTool.RequiresHandle = false
			TpTool.Parent = player.Backpack
			TpTool.Activated:Connect(function()
				local char = player.Character or workspace:FindFirstChild(player.Name)
				local hrp = char and char:FindFirstChild("HumanoidRootPart")
				if not char or not hrp then return end
				local screenPos
				if UIS.TouchEnabled then
					local touches = UIS:GetTouches()
					if #touches > 0 then
						screenPos = touches[1].Position
					end
				else
					screenPos = UIS:GetMouseLocation()
				end
				if not screenPos then return end
				local camera = workspace.CurrentCamera
				local ray = camera:ScreenPointToRay(screenPos.X, screenPos.Y)
				local params = RaycastParams.new()
				params.FilterDescendantsInstances = {char}
				params.FilterType = Enum.RaycastFilterType.Blacklist
				local result = workspace:Raycast(ray.Origin, ray.Direction * 1000, params)
				local targetPos = result and result.Position or (ray.Origin + ray.Direction * 50)
				hrp.CFrame = CFrame.new(targetPos + Vector3.new(0, 3, 0))
			end)
		end)		
	end,
})

-- {{{{{{{{{{  Start Loop  }}}}}}}}}} -- 

task.spawn(function()
	RunService.RenderStepped:Connect(function()
		pcall(function()
			if Enable_Movement then
				pcall(function()
					humanoid.WalkSpeed = WalkSpeed_st
					humanoid.JumpPower = JumpPower_st
					humanoid.JumpHeight = JumpPower_st
				end)
			end
		end)
	end)
	while true do
		if Esp then
			wait(0.5)
			pcall(function()
				for _, Model in ipairs(workspace:WaitForChild("Gameplay"):WaitForChild("Dynamic"):WaitForChild("Animals"):GetChildren()) do
					if Model:FindFirstChild("SomtankESP") then return end
					local Highlight = Instance.new("Highlight", Model)
					Highlight.OutlineColor = Color3.new(1, 1, 1)
					Highlight.FillTransparency = 1
					Highlight.OutlineTransparency = 0
					Highlight.Name = "SomtankESP"
				end
			end)
			for i=1,5 do
				if Esp then
					wait(1)
				end
			end
		end
		wait(0.3)
	end
end)


